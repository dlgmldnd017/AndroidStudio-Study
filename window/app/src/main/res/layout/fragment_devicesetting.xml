<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6F6"
    tools:context=".ui.deviceSetting.DeviceSettingFragment">

    <TextView
        android:id="@+id/deviceSetting_textView1"
        android:layout_width="225dp"
        android:layout_height="137dp"
        android:layout_marginStart="93dp"
        android:layout_marginLeft="93dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="93dp"
        android:layout_marginRight="93dp"
        android:autoText="false"
        android:background="#F6F6F6"
        android:duplicateParentState="false"
        android:editable="false"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deviceSetting_button2" />

    <ImageButton
        android:id="@+id/deviceSetting_button2"
        android:layout_width="76dp"
        android:layout_height="67dp"

        android:layout_marginStart="167dp"
        android:layout_marginLeft="167dp"
        android:layout_marginTop="46dp"
        android:layout_marginEnd="168dp"
        android:layout_marginRight="168dp"
        android:background="#F6F6F6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_popup_sync" />

    <Button
        android:id="@+id/deviceSetting_button1"
        android:layout_width="match_parent"
        android:layout_height="351dp"
        android:layout_marginTop="56dp"
        android:alpha=".100"
        android:autoText="false"
        android:background="#F6F6F6"
        android:enabled="true"
        android:fadeScrollbars="false"
        android:foreground="#F6F6F6"
        android:freezesText="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/deviceSetting_textView1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="168dp"
        android:layout_height="43dp"
        android:gravity="center"
        android:text="기기 추가하기"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="171dp"
        android:layout_height="157dp"
        android:layout_marginTop="24dp"
        android:gravity="center"
        android:shadowColor="#FFFFFF"
        android:text="+"
        android:textColor="#000000"
        android:textSize="100dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/deviceSetting_button1" />

</androidx.constraintlayout.widget.ConstraintLayout>